/* This tests growing of hash table */
a = generate[program="RECORDCOUNT AS ignore, RECORDCOUNT/5 AS a, CAST(RECORDCOUNT AS INTEGER) AS b", numRecords=23000];
b = hash_group_by[key="a", output="a, SUM(b) AS total"];
c = sort[key="a"];
d = write[file="output.txt", mode="text"];
a -> b;
b -> c;
c -> d;
