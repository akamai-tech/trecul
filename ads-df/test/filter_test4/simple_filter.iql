a = generate[program="RECORDCOUNT AS ignore, CASE WHEN RECORDCOUNT%5=1 THEN NULL ELSE RECORDCOUNT/5 END AS a, CAST(RECORDCOUNT AS INTEGER) AS b", numRecords=23];
cp = copy[output="input.*",output="input.*",output="input.*"];
a -> cp;
b1 = filter[where="a=3"];
c1 = sort[key="a"];
d1 = write[file="output1.txt", mode="text"];
cp -> b1;
b1 -> c1;
c1 -> d1;
b2 = filter[where="a<>3"];
c2 = sort[key="a"];
d2 = write[file="output2.txt", mode="text"];
cp -> b2;
b2 -> c2;
c2 -> d2;
b3 = filter[where="a IS NULL"];
c3 = sort[key="b"];
d3 = write[file="output3.txt", mode="text"];
cp -> b3;
b3 -> c3;
c3 -> d3;
